{
  "https://onibi.ru/manga/\\.*": {
    "parent": {
      "tag": "li",
      "attrs": {
        "class": "wp-manga-chapter"
      }
    },
    "link": {
      "tag": "a"
    }
  },
  "https://ficbook.net/fanfiction/\\w+/[\\w_]+": {
    "parent": {
      "tag": "article",
      "attrs": {
        "class": "block"
      }
    },
    "link": {
      "tag": "h3"
    },
    "text": {
      "tag": "div",
      "attrs": {
        "class": "description"
      }
    },
    "paginator": {
      "name": "p",
      "count": 2
    }
  },
  "https://pikabu.ru/@\\w+": {
    "parent": {
      "tag": "div",
      "attrs": {
        "class": "story__main"
      }
    },
    "link": {
      "tag": "h2"
    },
    "title": {
      "tag": "header"
    },
    "text": {
      "tag": "div",
      "attrs": {
        "class": "story__content-inner"
      }
    },
    "slice": {
      "end": -1
    },
    "parser": "html.parser"
  }
}
